{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "yes:rpg_blast",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particle/muzzle_blast"
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "v.life = 0;",
				"per_update_expression": "v.life = v.life+1;"
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 400,
				"max_particles": 90
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 1
			},
			"minecraft:emitter_shape_point": {
				"direction": [7, "Math.random(-4,4)", "Math.random(-4,4)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "12 / Math.Random(3, 8) + 2.5"
			},
			"minecraft:particle_initial_speed": "Math.random(2,15)",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [0.1, 0.4, 0.1],
				"linear_drag_coefficient": "v.life < 68 ? 5-(v.life/15) : 0.5"
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["variable.particle_random_1 * variable.particle_random_2 * 0.6 + 0.25 + Math.pow(v.life/120,2)", "variable.particle_random_1 * variable.particle_random_2 * 0.6 + 0.25 + Math.pow(v.life/120,2)"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 64,
					"texture_height": 64,
					"flipbook": {
						"base_UV": [56, 0],
						"size_UV": [8, 8],
						"step_UV": [-8, 0],
						"frames_per_second": 8,
						"max_frame": 8,
						"stretch_to_lifetime": true
					}
				}
			},
			"minecraft:particle_motion_collision": {
				"coefficient_of_restitution": 1,
				"collision_radius": 0.1
			},
			"minecraft:particle_appearance_lighting": {},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#80DADADA",
						"0.41": "#1AFFFFFF",
						"0.9": "#0D8E8E8E"
					}
				}
			}
		}
	}
}