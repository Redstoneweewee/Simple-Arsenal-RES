						{ "desert_eagle_hipfire_to_reload": 				"query.is_item_name_any('slot.weapon.mainhand', 'yes:desert_eagle', 'yes:empty_desert_eagle')" },
						{ "ak47_hipfire_to_reload": 						"query.is_item_name_any('slot.weapon.mainhand', 'yes:mk13', 'yes:empty_mk13')" },
						{ "rpg7_hipfire_to_reload": 						"query.is_item_name_any('slot.weapon.mainhand', 'yes:rpg7', 'yes:empty_rpg7')" },
						{ "ak47_hipfire_to_reload": 						"query.is_item_name_any('slot.weapon.mainhand', 'yes:ump45', 'yes:empty_ump45')" },
