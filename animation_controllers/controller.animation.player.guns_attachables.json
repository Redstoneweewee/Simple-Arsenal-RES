{
	"format_version" : "1.10.0",
	"animation_controllers" : {
		"controller.animation.player.guns_attachables" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
					"transitions" : [
						{
							"ak47_hold_state" : "variable.ak47_hold"
						},
						{
							"ak47_hipfire_state" : "(variable.ak47_hipfire || variable.holding_explosive_weapon)"
						},
						{
							"ak47_ads_state" : "variable.ak47_ads"
						}
					]
				},




				"ak47_hold_state" : {
					"animations" : [
						"hide_magazine"
					],
					"transitions" : [
						{
							"ak47_hipfire_state" : "variable.ak47_hipfire || variable.holding_explosive_weapon"
						},
						{
							"ak47_ads_state" : "variable.ak47_ads"
						},
						{
							"ak47_hold_to_reload_state" : "v.reloading_int == 1 && variable.ak47_hold"
						},
						{
							"default" : "!variable.ak47_hold"
						}
					]
				},
				"ak47_ads_to_hold_state" : {
					"animations" : [
						"hide_magazine"
					],
					"transitions" : [
						{
							"ak47_ads_state" : "variable.ak47_ads"
						},
						{
							"ak47_hipfire_state" : "variable.ak47_hipfire || variable.holding_explosive_weapon"
						},
						{
							"ak47_hold_to_reload_state" : "v.reloading_int == 1 && variable.ak47_hold"
						},
						{
							"default" : "!variable.ak47_hold"
						}
					]
				},
				"ak47_hipfire_to_hold_state" : {
					"animations" : [
						"hide_magazine"
					],
					"transitions" : [
						{
							"ak47_ads_state" : "variable.ak47_ads"
						},
						{
							"ak47_hipfire_state" : "variable.ak47_hipfire || variable.holding_explosive_weapon"
						},
						{
							"ak47_hold_to_reload_state" : "v.reloading_int == 1 && variable.ak47_hold"
						},
						{
							"default" : "!variable.ak47_hold"
						}
					]
				},







				"ak47_hipfire_state" : {
					"animations" : [
						"hide_magazine"
					],
					"transitions" : [
						{
							"ak47_hipfire_to_reload_state" : "(v.reloading_int == 1 && variable.ak47_hipfire) || (v.reloading_int == 1 && variable.holding_explosive_weapon)"
						},
						{
							"ak47_hipfire_to_hold_state" : "variable.ak47_hold"
						},
						{
							"ak47_hipfire_to_ads_state" : "variable.ak47_ads"
						},
						{
							"default" : "!variable.ak47_hipfire && !variable.holding_explosive_weapon"
						}
					]
				},
				"ak47_hipfire_to_hipfire_state" : {
					"animations" : [
						"hide_magazine"
					],
					"transitions" : [
						{
							"ak47_hipfire_to_reload_state" : "(v.reloading_int == 1 && variable.ak47_hipfire) || (v.reloading_int == 1 && variable.holding_explosive_weapon)"
						},
						{
							"ak47_hipfire_to_hold_state" : "variable.ak47_hold"
						},
						{
							"ak47_hipfire_to_ads_state" : "variable.ak47_ads"
						},
						{
