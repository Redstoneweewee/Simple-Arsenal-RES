						{ "remington870_hipfire_to_reload_normalize": 		"query.equipped_item_any_tag('slot.weapon.mainhand', 'yes:remington870') && !query.is_item_name_any('slot.weapon.mainhand', 'yes:remington870_5', 'yes:remington870_6') && c.is_first_person" },
						{ "remington870_hipfire_to_reload_last_normalize": 	"v.shotgun_cont_int == 0 && query.is_item_name_any('slot.weapon.mainhand', 'yes:remington870_5', 'yes:remington870_6') && c.is_first_person" },
						{ "remington870_reload_last_cont_normalize": 		"v.shotgun_cont_int >= 1 && query.is_item_name_any('slot.weapon.mainhand', 'yes:remington870_5', 'yes:remington870_6') && c.is_first_person" },
